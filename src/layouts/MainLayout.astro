---
import Head from '../components/Head.astro'
import Header from '../components/Header.astro'

export interface Props {
  title: string
}

const { title } = Astro.props as Props
---

<html lang="en">
  <Head {title} />
  <body class="">
    <Header />
    <slot />
  </body><style is:global>
    [x-cloak] {
      display: none !important;
    }
  </style>
</html>
